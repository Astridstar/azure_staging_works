{
	"name": "processVehicleInfo",
	"properties": {
		"activities": [
			{
				"name": "Get trigger information",
				"description": "Get meta info",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.00:10:00",
					"retry": 1,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "VehicleSet",
						"type": "DatasetReference",
						"parameters": {
							"directory": "@pipeline().parameters.file",
							"container": "@pipeline().parameters.directory"
						}
					},
					"fieldList": [
						"itemName"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			}
		],
		"concurrency": 1,
		"parameters": {
			"directory": {
				"type": "string",
				"defaultValue": "@triggerBody.folderPath"
			},
			"file": {
				"type": "string",
				"defaultValue": "@triggerBody().fileName"
			}
		},
		"annotations": []
	}
}