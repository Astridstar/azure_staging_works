{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ingestion-factory"
		},
		"VehLandingZoneStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synsqq267aknc6xi.dfs.core.windows.net/"
		},
		"VehLandingZoneVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvsqq267aknc6xi.vault.azure.net/"
		},
		"VehLandindZoneIngestion_properties_processVehicleInfo_parameters_directory": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"VehLandindZoneIngestion_properties_processVehicleInfo_parameters_file": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"VehLandindZoneIngestion_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0898411e-cf58-45f1-822f-5b65523e4b6d/resourceGroups/gilgamesh/providers/Microsoft.Storage/storageAccounts/synsqq267aknc6xi"
		},
		"VehicleStagedZoneStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synsqq267aknc6xi.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ingestion-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"VehLandingZoneVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('VehLandingZoneVault_properties_typeProperties_baseUrl')]"
					},
					"VehLandingZoneStorage_properties_typeProperties_url": {
						"value": "[parameters('VehLandingZoneStorage_properties_typeProperties_url')]"
					},
					"VehicleStagedZoneStorage_properties_typeProperties_url": {
						"value": "[parameters('VehicleStagedZoneStorage_properties_typeProperties_url')]"
					},
					"VehLandindZoneIngestion_properties_processVehicleInfo_parameters_directory": {
						"value": "[parameters('VehLandindZoneIngestion_properties_processVehicleInfo_parameters_directory')]"
					},
					"VehLandindZoneIngestion_properties_processVehicleInfo_parameters_file": {
						"value": "[parameters('VehLandindZoneIngestion_properties_processVehicleInfo_parameters_file')]"
					},
					"VehLandindZoneIngestion_properties_typeProperties_scope": {
						"value": "[parameters('VehLandindZoneIngestion_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}