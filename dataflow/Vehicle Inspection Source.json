{
	"name": "Vehicle Inspection Source",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "VehicleInspection",
						"type": "DatasetReference"
					},
					"name": "VehicleInspectionSource"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          MONTH_REPORTED as string,",
				"          STATE as string,",
				"          MAKE as string,",
				"          MODEL as string,",
				"          COLOR as string,",
				"          YEAR as short,",
				"          LAST_INSPECTION_MONTH as string,",
				"          NUMBER_OF_TRIPS as short,",
				"          MULTIPLE_TNPS as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 10,",
				"     ignoreNoFilesFound: true,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: false,",
				"     rowUrlColumn: 'FILENAME') ~> VehicleInspectionSource",
				"VehicleInspectionSource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1) ~> sink1"
			]
		}
	}
}